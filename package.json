{
  "name": "commitlint-plugin-spend",
  "version": "1.0.1",
  "description": "A commitlint plugin that requires the GitLab /spend or /spend_time directive in commit messages",
  "main": "out/index.js",
  "type": "module",
  "keywords": [
    "commitlint",
    "commitlintplugin",
    "git",
    "lab",
    "commit",
    "spend",
    "message",
    "time",
    "tracking"
  ],
  "author": "Nico Ismaili",
  "license": "MIT",
  "homepage": "https://github.com/nico-i/commitlint-plugin-spend",
  "repository": {
    "type": "git",
    "url": "https://github.com/nico-i/commitlint-plugin-spend.git"
  },
  "bugs": {
    "url": "https://github.com/nico-i/commitlint-plugin-spend/issues"
  },
  "scripts": {
    "build": "bun build ./index.ts --outdir ./out",
    "test": "bun test --coverage",
    "prepare": "husky"
  },
  "peerDependencies": {
    "@commitlint/lint": "19.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@commitlint/core": "19.8.1",
    "@commitlint/types": "19.8.1",
    "@types/bun": "1.2.15",
    "@types/node": "22.15.29",
    "husky": "9.1.7"
  },
  "private": true
}
